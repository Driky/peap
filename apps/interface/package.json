{
  "name": "interface",
  "version": "1.0.0",
  "description": "A poc implementation of Phoenix Elixir Angular Postgres and few other things.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-client-unit": "bin/run_client_unit_tests.sh",
    "test": "mix test && npm run test-client-unit",
    "clean": "rm -rf priv/static/*",
    "copy": "cp web/static/* priv/static",
    "copy-watch": "node_modules/.bin/cpx \"web/static/*\" priv/static --watch",
    "webpack": "node_modules/.bin/webpack",
    "webpack-watch": "node_modules/.bin/webpack --watch-stdin --progress --colors",
    "webpack-test": "node_modules/.bin/webpack --config webpack-test.config.js",
    "webpack-test-e2e": "node_modules/.bin/webpack --config webpack-test-e2e.config.js",
    "typings": "typings",
    "postinstall": "typings install",
    "start": "mix phoenix.server"
  },
  "author": "Adam Keating",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.3.1",
    "@angular/compiler": "2.3.1",
    "@angular/core": "2.3.1",
    "@angular/forms": "2.3.1",
    "@angular/http": "2.3.1",
    "@angular/platform-browser": "2.3.1",
    "@angular/platform-browser-dynamic": "2.3.1",
    "@angular/router": "3.3.1",
    "bootstrap": "4.0.0-alpha.2",
    "es6-shim": "0.35.2",
    "font-awesome": "4.7.0",
    "phoenix": "1.2.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "3.0.0-beta.17",
    "chai": "3.5.0",
    "cpx": "1.5.0",
    "css-loader": "^0.25",
    "css-to-string-loader": "0.1.2",
    "file-loader": "0.9.0",
    "globby": "6.1.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "4.0.0",
    "protractor": "4.0.13",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "1.6.1",
    "sass-loader": "4.1.0",
    "style-loader": "0.13.1",
    "typescript": "2.1.4",
    "typings": "2.0.0",
    "webpack": "2.1.0-beta.28",
    "url-loader": "0.5.7"
  }
}
