{
  "name": "peap_demo",
  "version": "1.0.0",
  "description": "Seed Phoneixframework app with Webpack and Postgres",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf priv/static/*",
    "copy": "cp web/static/root/* priv/static",
    "copy-watch": "node_modules/.bin/cpx \"web/static/root/*\" priv/static --watch",
    "webpack": "node_modules/.bin/webpack",
    "webpack-watch": "node_modules/.bin/webpack --watch-stdin --progress --colors",
    "build": "npm run clean && npm run copy && npm run webpack",
    "build-min": "npm run clean && npm run copy && npm run webpack --optimize-minimize",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "author": "Adam Keating",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "font-awesome": "^4.6.1",
    "phoenix": "file:deps/phoenix",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.17.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "cpx": "^1.3.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "url-loader": "^0.5.7",
    "val-loader": "^0.5.0",
    "webpack": "^1.13.0"
  }
}
